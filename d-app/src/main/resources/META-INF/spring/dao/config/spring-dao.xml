			value="#{T(com.finaxys.rd.dataextraction.domain.msg.Document.ContentType).XLS}" />
	</bean>
	<bean id="fileInterbankRateGateway"
		class="com.finaxys.rd.dataextraction.dao.integration.file.FileInterbankRateGateway">
